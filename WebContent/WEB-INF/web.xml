<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
		version="4.0">
  <display-name>delFirstTomcat</display-name>
  <welcome-file-list>
    <welcome-file>firstTomcat</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>addServlet</servlet-name>
  	<servlet-class>site.laneinline.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addServlet</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>sqrServlet</servlet-name>
  	<servlet-class>site.laneinline.SqrServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>sqrServlet</servlet-name>
  	<url-pattern>/sqr</url-pattern>
  </servlet-mapping>
  
</web-app>