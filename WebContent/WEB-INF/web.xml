<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
		version="4.0">
  <display-name>delFirstTomcat</display-name>
  <welcome-file-list>
    <welcome-file>firstTomcat</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>addServlet</servlet-name>
  	<servlet-class>site.laneinline.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addServlet</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>sqrServlet</servlet-name>
  	<servlet-class>site.laneinline.SqrServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>sqrServlet</servlet-name>
  	<url-pattern>/sqr</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>myServlet</servlet-name>
  	<servlet-class>site.laneinline.MyServlet</servlet-class>
  	
  	<init-param>
  		<param-name>name </param-name> 
  		<param-value>lane2line</param-value> 
  	</init-param>
  	
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>myServlet</servlet-name>
  	<url-pattern>/mysr</url-pattern>
  </servlet-mapping>
  
  <context-param>
  	<param-name>name</param-name>
  	<param-value>laneinline</param-value>
  </context-param>
  
   <context-param>
  	<param-name>phone</param-name>
  	<param-value>Android</param-value>
  </context-param>
  
  <context-param>
  	<param-name>myParam</param-name>
  	<param-value>randomString</param-value>
  </context-param>
</web-app>